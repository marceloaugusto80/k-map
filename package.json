{
  "name": "k-map",
  "version": "0.0.1",
  "description": "Plot placemarks from kml files in a map.",
  "author": "Marcelo Augusto R. A.",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "test": "npx jest --config jest.config.js --no-cache",
    "build-renderer": "npx webpack -c webpack.renderer.config.js",
    "build-main": "npx webpack -c webpack.main.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-electron": "npx electron ./dist/main.js",
    "watch-renderer": "npx webpack -c webpack.renderer.config.js --watch",
    "start": "npm run build && npx concurrently --kill-others \"npm:watch-renderer\" \"npm:start-electron\"",
    "pack": "npm install && npm run build && npx electron-packager ./dist --out ./out --overwrite"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^17.0.1",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/ol": "^6.4.2",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-image-crop": "^8.1.2",
    "@types/sax": "^1.2.1",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.2",
    "electron-packager": "^15.2.0",
    "electron-reloader": "^1.2.0",
    "html-webpack-plugin": "^5.2.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-each": "^26.6.2",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.16",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "electron": "^11.3.0",
    "html2canvas": "^1.0.0-rc.7",
    "img-clipboard": "^1.0.4",
    "lodash": "^4.17.21",
    "ol": "^6.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-image-crop": "^8.6.6",
    "sax": "^1.2.4"
  }
}
